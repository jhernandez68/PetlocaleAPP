    <TextView
        android:id="@+id/tipoComparacionProducto3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto2"
        app:layout_constraintTop_toBottomOf="@+id/nombreComparacionProducto4" />

    <TextView
        android:id="@+id/tipoComparacionProducto4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nombreComparacionProducto3"
        app:layout_constraintTop_toBottomOf="@+id/nombreComparacionProducto4" />

    <TextView
        android:id="@+id/costoComparacionProducto3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Costo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto2"
        app:layout_constraintTop_toBottomOf="@+id/tipoComparacionProducto3" />

    <TextView
        android:id="@+id/costoComparacionProducto4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="precio"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/costoComparacionProducto3"
        app:layout_constraintTop_toBottomOf="@+id/tipoComparacionProducto4" />


    <TextView
        android:id="@+id/categoriaComparacionProducto3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto2"
        app:layout_constraintTop_toBottomOf="@+id/costoComparacionProducto4" />

    <TextView
        android:id="@+id/categoriaComparacionProducto4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/categoriaComparacionProducto3"
        app:layout_constraintTop_toBottomOf="@+id/costoComparacionProducto3" />

    <TextView
        android:id="@+id/nombreVeterinariaComparacionProducto3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Veterinaria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto2"
        app:layout_constraintTop_toBottomOf="@+id/categoriaComparacionProducto3" />

    <TextView
        android:id="@+id/nombreVeterinariaComparacionProducto4"
