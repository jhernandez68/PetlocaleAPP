        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/categoriaComparacionProducto"
        app:layout_constraintTop_toBottomOf="@+id/costoComparacionProducto1" />

    <TextView
        android:id="@+id/nombreVeterinariaComparacionProducto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
