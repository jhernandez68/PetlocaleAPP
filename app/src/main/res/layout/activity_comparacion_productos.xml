        app:layout_constraintStart_toEndOf="@+id/calificacionComparacionProducto1"
        app:layout_constraintTop_toBottomOf="@+id/nombreVeterinariaComparacionProducto" />

    <TextView
        android:id="@+id/nit_comparacion_producto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
