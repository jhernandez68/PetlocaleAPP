        android:id="@+id/calificacionMejorPrecioProducto2"
        app:layout_constraintTop_toBottomOf="@+id/imagenProducto2"
        app:layout_constraintStart_toEndOf="@id/calificacionMejorPrecioProducto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Un producto sin calificación"
        android:textColor="#000000"
         />

    <TextView
        android:id="@+id/calificacionMejorCostoProducto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Más barato: "
        android:textColor="#572364"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calificacionMejorPrecioProducto" />

    <TextView
        android:id="@+id/calificacionMejorCostoProducto2"
        app:layout_constraintTop_toBottomOf="@+id/calificacionMejorPrecioProducto"
        app:layout_constraintStart_toEndOf="@id/calificacionMejorCostoProducto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Un producto sin calificación"
        android:textColor="#000000"
        />

