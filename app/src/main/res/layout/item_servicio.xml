<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp">

    <ImageView
        android:id="@+id/ivServicio"
        android:layout_width="144dp"
        android:layout_height="130dp"
        app:srcCompat="@mipmap/petlocaleicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_servicio_detailed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#572364"
        android:text="Nombre: "
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_servicio_detailed2XD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/main_name_servicio_detailed"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_vet_servicio_detailed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toBottomOf="@+id/main_name_servicio_detailed2XD" />

    <TextView
        android:id="@+id/main_name_vet_servicio_detailed2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/main_name_vet_servicio_detailed"
        app:layout_constraintTop_toBottomOf="@+id/main_name_servicio_detailed2XD" />

    <TextView
        android:id="@+id/mascotasUserInfoServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Costo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toBottomOf="@+id/main_name_vet_servicio_detailed2" />

    <TextView
        android:id="@+id/yearsDetailed2Servicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="precio"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/mascotasUserInfoServicio"
        app:layout_constraintTop_toBottomOf="@+id/main_name_vet_servicio_detailed2" />


    <TextView
        android:id="@+id/emailDetailedServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripción: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toBottomOf="@+id/yearsDetailed2Servicio" />

    <TextView
        android:id="@+id/emailDetailed2Servicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripción"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/emailDetailedServicio"
        app:layout_constraintTop_toBottomOf="@+id/yearsDetailed2Servicio" />

    <TextView
        android:id="@+id/nameVeterinariaXDServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Veterinaria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toBottomOf="@+id/emailDetailedServicio" />

    <TextView
        android:id="@+id/nameVeterinaria2XDServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre veterinaria"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nameVeterinariaXDServicio"
        app:layout_constraintTop_toBottomOf="@+id/emailDetailed2Servicio" />

    <TextView
        android:id="@+id/categoriaServicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivServicio"
        app:layout_constraintTop_toBottomOf="@+id/nameVeterinariaXDServicio" />

    <TextView
        android:id="@+id/categoriaServicio2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria "
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/categoriaServicio"
        app:layout_constraintTop_toBottomOf="@+id/nameVeterinariaXDServicio" />

    <TextView
        android:id="@+id/nit_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="110dp" />

</androidx.constraintlayout.widget.ConstraintLayout>