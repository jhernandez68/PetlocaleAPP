<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComparacionProductos">

    <ImageView
        android:id="@+id/imagenProducto1"
        android:layout_width="143dp"
        android:layout_height="152dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/petlocaleicon" />

    <TextView
        android:id="@+id/nombreComparacionProducto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#572364"
        android:text="Nombre: "
        app:layout_constraintStart_toEndOf="@+id/imagenProducto1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nombreComparacionProducto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nombreComparacionProducto1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tipoComparacionProducto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto1"
        app:layout_constraintTop_toBottomOf="@+id/nombreComparacionProducto2" />

    <TextView
        android:id="@+id/tipoComparacionProducto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nombreComparacionProducto1"
        app:layout_constraintTop_toBottomOf="@+id/nombreComparacionProducto2" />

    <TextView
        android:id="@+id/costoComparacionProducto1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Costo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/imagenProducto1"
        app:layout_constraintTop_toBottomOf="@+id/tipoComparacionProducto1" />

    <TextView
        android:id="@+id/costoComparacionProducto2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="precio"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/costoComparacionProducto1"
        app:layout_constraintTop_toBottomOf="@+id/tipoComparacionProducto2" />


    <TextView
