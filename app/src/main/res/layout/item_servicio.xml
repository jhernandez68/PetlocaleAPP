<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp">

    <ImageView
        android:id="@+id/ivProducto"
        android:layout_width="144dp"
        android:layout_height="130dp"
        app:srcCompat="@mipmap/petlocaleicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_product_detailed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#572364"
        android:text="Nombre: "
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_product_detailed2XD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/main_name_product_detailed"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/main_name_vet_product_detailed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toBottomOf="@+id/main_name_product_detailed2XD" />

    <TextView
        android:id="@+id/main_name_vet_product_detailed2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/main_name_vet_product_detailed"
        app:layout_constraintTop_toBottomOf="@+id/main_name_product_detailed2XD" />

    <TextView
        android:id="@+id/mascotasUserInfo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Costo: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toBottomOf="@+id/main_name_vet_product_detailed2" />

    <TextView
        android:id="@+id/yearsDetailed2Product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="precio"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/mascotasUserInfo"
        app:layout_constraintTop_toBottomOf="@+id/main_name_vet_product_detailed2" />


    <TextView
        android:id="@+id/emailDetailedProduct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripción: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toBottomOf="@+id/yearsDetailed2Product" />

    <TextView
        android:id="@+id/emailDetailed2Product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripción"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/emailDetailedProduct"
        app:layout_constraintTop_toBottomOf="@+id/yearsDetailed2Product" />

    <TextView
        android:id="@+id/nameVeterinariaXD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Veterinaria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toBottomOf="@+id/emailDetailedProduct" />

    <TextView
        android:id="@+id/nameVeterinaria2XD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre veterinaria"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/nameVeterinariaXD"
        app:layout_constraintTop_toBottomOf="@+id/emailDetailed2Product" />

    <TextView
        android:id="@+id/categoriaService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria: "
        android:textColor="#572364"
        app:layout_constraintStart_toEndOf="@+id/ivProducto"
        app:layout_constraintTop_toBottomOf="@+id/nameVeterinariaXD" />

    <TextView
        android:id="@+id/categoriaService2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria "
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/categoriaService"
        app:layout_constraintTop_toBottomOf="@+id/nameVeterinariaXD" />

    <TextView
        android:id="@+id/nit_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="110dp" />

</androidx.constraintlayout.widget.ConstraintLayout>