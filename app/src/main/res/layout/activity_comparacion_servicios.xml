    <TextView
        android:id="@+id/nombreComparacionServicio1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#572364"
        android:text="Nombre: "
        app:layout_constraintStart_toEndOf="@+id/imagenServicio1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nombreComparacionServicio2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
